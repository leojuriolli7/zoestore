{
  "name": "zoestore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:nginx\" \"npm run dev:next\"",
    "dev:nginx": "brew services start nginx && echo 'Nginx started!'",
    "dev:next": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d",
    "db:studio": "npx drizzle-kit studio",
    "db:push": "npx drizzle-kit push"
  },
